{
  "name": "findologic/plentymarkets-rest-export",
  "type": "library",
  "description": "FINDOLOGIC plugin for Plentymarkets",
  "keywords": [
    "Plentymarkets",
    "FINDOLOGIC"
  ],
  "require": {
    "php": ">=7.2",
    "findologic/http_request2": "2.3.1",
    "vasily-kartashov/log4php": "4.1.0"
  },
  "require-dev": {
    "mikey179/vfsStream": "^1.6",
    "phpunit/phpunit": "8.0.2"
  },
  "autoload": {
    "psr-4": {
      "Findologic\\Plentymarkets\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:vasily-kartashov/log4php.git"
    }
  ],
  "scripts": {
    "run-tests": "phpunit --configuration phpunit.xml"
  }
}
