{
  "name": "findologic-plentymarkets",
  "type": "application",
  "description": "Findologic plugin for Plentymarkets",
  "keywords": [
    "plentymarkets",
    "findologic"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:findologic/plentymarkets-rest-export.git",
      "no-api": true
    }
  ],
  "minimal-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "pear/http_request2": "^2.3",
    "apache/log4php": "2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Findologic\\Plentymarkets\\": "src/"
    }
  }
}
